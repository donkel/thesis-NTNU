	file = {Rose et al. - 2020 - Zero Trust Architecture.pdf:/home/torstein/snap/zotero-snap/common/Zotero/storage/2UH7GA33/Rose et al. - 2020 - Zero Trust Architecture.pdf:application/pdf},
}

